library(multipathaic)
set.seed(123)
X <- matrix(rnorm(100 * 7), 100, 7)
colnames(X) <- paste0("x", 1:7)
# True model: y depends on x1 and x2
y <- 2*X[,1] + 1.5*X[,2] + rnorm(100)
X
y
paths <- build_paths(
X = X,
y = y,
family = "gaussian",
K = 5,           # Try up to 5 variables
delta = 1.5,     # Keep models within 1.5 AIC of best
verbose = FALSE
)
paths
print("")
print("Models at each step:")
for (k in 1:length(paths$frontiers)) {
n_models <- length(paths$frontiers[[k]])
best_aic <- min(sapply(paths$frontiers[[k]], function(m) m$aic))
print(sprintf("  Step %d: %d models, Best AIC = %.2f", k, n_models, best_aic))
}
print("")
print("================================================================================")
print("STEP 2: STABILITY ANALYSIS (Bootstrap)")
print("================================================================================")
stab <- stability(
X = X,
y = y,
family = "gaussian",
B = 30,          # 30 bootstrap samples
K = 5,
verbose = FALSE
)
stab
sorted_stab <- sort(stab$pi, decreasing = TRUE)
for (i in 1:length(sorted_stab)) {
var_name <- names(sorted_stab)[i]
stability <- sorted_stab[i]
# Classify stability
if (stability >= 0.8) {
label <- "VERY STABLE"
} else if (stability >= 0.6) {
label <- "STABLE"
} else if (stability >= 0.4) {
label <- "MODERATE"
} else {
label <- "UNSTABLE"
}
print(sprintf("  %s: %.3f (%s)", var_name, stability, label))
}
plausible <- plausible_models(
path_result = paths,
stability_result = stab,
Delta = 2,       # Within 2 AIC of best
tau = 0.6        # Average stability >= 0.6
)
plausible
# Get all models
all_models <- extract_all_models(paths)
all_models
print("")
print(sprintf("Total unique models explored: %d", nrow(all_models)))
# Show best model at each step
print("")
print("Best model at each step:")
for (k in 1:max(all_models$step)) {
step_models <- all_models[all_models$step == k, ]
best_idx <- which.min(step_models$aic)
best_model <- step_models[best_idx, ]
print(sprintf("  Step %d: %s (AIC = %.2f)",
k, best_model$variables, best_model$aic))
}
# Find min AIC
min_aic <- min(all_models$aic)
print(sprintf("  2. Minimum AIC: %.2f", min_aic))
# How many within Delta=2 of min AIC
aic_filtered <- all_models[all_models$aic <= min_aic + 2, ]
print(sprintf("  3. Models within Delta=2 of min AIC: %d", nrow(aic_filtered)))
# How many pass stability threshold
print(sprintf("  4. Models with avg stability >= 0.6: %d", nrow(plausible)))
print("")
print("Final Plausible Models (Details):")
for (i in 1:nrow(plausible)) {
print(sprintf("  Model %d:", i))
print(sprintf("    Variables: %s", plausible$variables[i]))
print(sprintf("    Size: %d predictors", plausible$size[i]))
print(sprintf("    AIC: %.2f (Delta from best: %.2f)",
plausible$aic[i], plausible$delta_aic[i]))
print(sprintf("    Avg Stability: %.3f", plausible$avg_stability[i]))
print("")
}
print("================================================================================")
print("VARIABLE IMPORTANCE RANKING")
print("================================================================================")
importance <- variable_importance_ranking(paths, stab)
print("")
print("Variables ranked by composite importance:")
print(importance)
print("")
print("Top 3 Most Important Variables:")
for (i in 1:min(3, nrow(importance))) {
print(sprintf("  %d. %s", i, importance$Variable[i]))
print(sprintf("     - Stability: %.3f", importance$Stability[i]))
print(sprintf("     - Appearance Count: %d", importance$AppearanceCount[i]))
print(sprintf("     - Importance Score: %.3f", importance$ImportanceScore[i]))
}
print("")
print("================================================================================")
print("CREATING VISUALIZATIONS")
print("================================================================================")
# Plot 1: Stability
plot_stability(stab, threshold = 0.6)
title(sub = "Variables above red line are stable (>= 0.6)", line = 2.5, cex.sub = 0.8)
# Plot 2: Model exploration
plot_model_tree(paths, max_models = 20)
# Plot 3: Dashboard
plot_model_dashboard(paths, stab, plausible)
# Plot 4: Variable importance
plot_variable_importance(importance, top_n = 7)
print("")
